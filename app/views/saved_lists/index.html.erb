<p id="notice"><%= notice %></p>

<h1>Saved Lists</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_lists.each do |saved_list| %>
      <tr>
        <td><%= saved_list.user %></td>
        <td><%= link_to 'Show', saved_list %></td>
        <td><%= link_to 'Edit', edit_saved_list_path(saved_list) %></td>
        <td><%= link_to 'Destroy', saved_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved List', new_saved_list_path %>
